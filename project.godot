; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Door",
"language": "GDScript",
"path": "res://scripts/door.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://scripts/player.gd"
}, {
"base": "StaticBody2D",
"class": "Shadow",
"language": "GDScript",
"path": "res://scripts/shadow.gd"
} ]
_global_script_class_icons={
"Door": "",
"Player": "",
"Shadow": ""
}

[application]

config/name="LD50"
run/main_scene="res://MainScene.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Globals.tscn"

[display]

window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/vnen.tiled_importer/plugin.cfg" )

[input]

advance_dialog={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":0,"physical_scancode":0,"unicode":0,"echo":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_2="Player"
2d_physics/layer_3="Shadow"
2d_physics/layer_4="Object"
2d_physics/layer_5="Wall"
2d_physics/layer_6="Hole"
2d_physics/layer_7="Nonstatic"
2d_physics/layer_8="TriggerAreas"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"

[tiled_importer]

enable_json_format=true
